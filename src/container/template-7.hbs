<body>
  <header class='header'>
    {{>headerDark data=header}}
   </header>
   <section class="hero">
    {{#with hero}}
    <h1 class="hero_title">{{title}}</h1>
    <p class="hero_description">{{description}}</p>
    <a href="{{button.url}}" class="hero_button">{{button.text}}</a>
    {{/with}}
   </section>
  <main class="main">
    {{#with about}}
      <h2 class="about_title">{{heading}}</h1>
      <p class="about_text">{{info}}</p>
    {{/with}}
  </main>
  <section class="blog">
   <div class="section_header">
    <h3 class="section_title">Our Articles</h3> 
    <a href="#" class="blog_button">View All</a>
    </div>
    <div class="card_container">
      {{>articleCardDark}}
    </div>
  </section>
  <footer class="footer">
    {{>footerDark}}
  </footer>
</body>